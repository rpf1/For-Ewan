<?xml version="1.0" encoding="utf-8"?><Defs>

  <SoundDef>
    <defName>GeyserSpray</defName>  
    <sustain>True</sustain>  
    <context>MapOnly</context>
    <priorityMode>PrioritizeNearest</priorityMode>  
    <sustainFadeoutTime>1</sustainFadeoutTime>  
    <subSounds>
      <li>
        <grains>
          <li Class="AudioGrain_Clip">
            <clipPath>Misc/Steam_Geyser/SteamGeyser_Venting</clipPath>
          </li>
        </grains>      
        <volumeRange>5~6</volumeRange>      
        <pitchRange>0.9088235~1</pitchRange>      
        <distRange>10~34</distRange>      
        <sustainAttack>2</sustainAttack>      
        <sustainSkipFirstAttack>False</sustainSkipFirstAttack>
